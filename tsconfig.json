{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2020",
    "sourceMap": false,
    "outDir": "dict",
    "removeComments": false,
    "strict": true,
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths": {
      "ROOT": [
        "./app.ts"
      ],
      "@*": [
        "src/*"
      ],
      "#*": [
        "src/plugins/*"
      ]
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "node_modules"
  ]
}